{
  "private": true,
  "name": "casinocoin-ledger-electron-hid",
  "version": "1.0.0",
  "description": "A CasinoCoin Ledger App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-rebuild",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": "https://github.com/sarathkaleswaram/ledger-live-electron-hid",
  "keywords": [
    "Electron",
    "CasinoCoin",
    "csc",
    "Ledger"
  ],
  "build": {
    "appId": "org.casinocoin-ledger.com",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "author": {
    "name": "CasinoCoin Foundation",
    "email": "info@casinocoin.org"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^20.44.4",
    "electron-rebuild": "^1.8.4"
  },
  "dependencies": {
    "@ledgerhq/hw-transport-node-hid": "^4.38.6",
    "babel-polyfill": "^6.26.0",
    "casinocoin-libjs": "^1.0.12",
    "hw-app-csc": "2.0.0",
    "jsonschema": "^1.2.2"
  },
  "resolutions": {
    "jsonschema": "1.2.2"
  }
}
